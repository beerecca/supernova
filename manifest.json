{
  "name": "Supernova",
  "version": "1.0",
  "description": "Let's make Nova a bit more usable",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "*://secure6.waytopay.nl/*",
    "*://secure.waytopay.nl/*"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/supernova16.png",
      "32": "images/supernova32.png",
      "48": "images/supernova48.png",
      "128": "images/supernova128.png"
    }
  },
  "icons": {
    "16": "images/supernova16.png",
    "32": "images/supernova32.png",
    "48": "images/supernova48.png",
    "128": "images/supernova128.png"
  },
  "web_accessible_resources": [
    "supernova.css",
    "fonts/Fakt/FaktPro-Normal.ttf",
    "fonts/Fakt/FaktPro-Medium.ttf"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://secure6.waytopay.nl/*",
        "*://secure.waytopay.nl/*"
      ],
      "js": [
        "supernova.js"
      ],
      "css": [
        "supernova.css"
      ]
    }
  ]
}


